{
  "env": {
  "browser": true,
  "es6": true,
  "node": true
  },
  "extends": [
  "airbnb",
  "airbnb/hooks",
  "plugin:jsx-a11y/recommended"
  ],
  "plugins": [
    "jsx-a11y"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/react-in-jsx-scope": "off",
    "no-console": "off"
  }
 }